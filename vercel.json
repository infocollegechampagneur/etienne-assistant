{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "MONGO_URL": "@mongo_url",
    "HUGGINGFACE_TOKEN": "@huggingface_token",
    "DB_NAME": "etienne_free"
  }
}
