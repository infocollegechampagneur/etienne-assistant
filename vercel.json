{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ],
  "functions": {
    "api/main.py": {
      "maxDuration": 30
    }
  }
}
