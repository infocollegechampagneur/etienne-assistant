{
  \"version\": 2,
  \"builds\": [
    {
      \"src\": \"api/index.py\",
      \"use\": \"@vercel/python\",
      \"config\": {
        \"maxLambdaSize\": \"50mb\"
      }
    },
    {
      \"src\": \"index.html\",
      \"use\": \"@vercel/static\"
    }
  ],
  \"routes\": [
    {
      \"src\": \"/api/(.*)\",
      \"dest\": \"api/index.py\"
    },
    {
      \"src\": \"/(.*)\",
      \"dest\": \"/index.html\"
    }
  ],
  \"functions\": {
    \"api/index.py\": {
      \"runtime\": \"python3.9\",
      \"maxDuration\": 30,
      \"memory\": 1024
    }
  }
}
